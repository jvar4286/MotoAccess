<div class="container col-lg-12 text-center">
<h1>Pública tu articulo</h1>
 
 <div class="page-scroll btn btn btn-xl ">
    <%= link_to 'New Publish', new_publish_path %>
  </div>

  <div class="page-scroll btn btn-xl">
  <%= link_to 'Destroy', destroy_user_session_path, method: :delete %>
  </div>
</div>

<div class="container">
  <p></p>
  <table class="table">
    <thead>
      <tr>
        <th>Make</th>
        <th>Description</th>
        <th>Price</th>
        <!--<th colspan="3"></th>-->
      </tr>
    </thead>
    <tbody>
    <% @publishes.each do |publish| %>
      <tr>
        <td><%= publish.make %></td>
        <td><%= publish.description %></td>
        <td><%= publish.price %></td>
      </tr>
      <div class="container col-lg-12 text-center">
      <tr>
        <td class="page-scroll btn btn btn-xm "><%= link_to 'Show', publish %></td>
    
        <td class="page-scroll btn btn btn-xm "><%= link_to 'Edit', edit_publish_path(publish) %></td>
        
        <td class="page-scroll btn btn btn-xm "><%= link_to 'Destroy', publish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      </div>
        <% end %>
    </tbody>
  </table>
</div>




